<br></br>
<div class="main">
  <div class="inner">
    <div class="form__wrapper">
      <h2 class="page-heading">お知らせ投稿</h2>
        <%= form_with model: print, local: true do |f|%>
          <div class="field">
            <%= f.label :title, "題名" %><br />
            <%= f.text_field :title %>
          </div>

          <div class="field">
            <%= f.label :text, "本文" %><br />
            <%= f.text_area :catch_copy, class: :form__text %>
          </div>

          <div class="field">
            <%= f.label :image, "画像" %><br />
            <%= f.file_field :image %>
          </div>

          <div class="actions">
            <%= f.submit "保存する", class: :form__btn  %>
          </div>
        <% end %>
    </div>
  </div>
</div>